{
  "name": "ghoneycutt-timezone",
  "version": "2.0.0",
  "author": "ghoneycutt",
  "summary": "Manage system time zone",
  "license": "Apache-2.0",
  "source": "git://github.com/ghoneycutt/puppet-module-timezone.git",
  "project_page": "https://github.com/ghoneycutt/puppet-module-timezone",
  "issues_url": "https://github.com/ghoneycutt/puppet-module-timezone/issues",
  "description": "Manage system time zone",
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 5.5.10 < 7.0.0" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "18.04"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 5.2.0 < 7.0.0"}
  ]
}
